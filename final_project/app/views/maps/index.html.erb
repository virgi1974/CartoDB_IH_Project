<%= javascript_include_tag 'usermaps/stored_maps.js' %>



<p id="<%= @user.id %>" class="btn btn-info show-stored-maps" style="display:none;" hidden></p>

<div class="container">
  <div class="row maps-gallery">
    
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2 ">
      <%= link_to 'Create a New Map', new_user_map_path(current_user), class: "btn btn-primary btn-block" %> 
      <br>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div id="selected-map" class="col-md-12 map-container studycase">
    </div>
  </div>
</div>
<br>
<% if current_user %>
 <div class="container-fluid">
    <header class="row">
      <section class="col-sm-12 well text-center">
        <p><strong>Name:</strong>  <%= current_user.name %> <strong>Email:</strong> <%= current_user.email %></p>
      </section>
    </header>
  </div>
<% end %>

